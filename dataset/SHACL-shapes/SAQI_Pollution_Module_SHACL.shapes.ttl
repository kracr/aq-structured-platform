AirQualityOntology:TrajectoryShape
  rdf:type rdfs:Class ;
  rdf:type sh:NodeShape ;
  rdfs:subClassOf rdfs:Resource ;
  sh:property [
      rdf:type sh:PropertyShape ;
      sh:path AirQualityOntology:hasPoint ;
      sh:class AirQualityOntology:Trajectory ;
      sh:minCount 1 ;
      sh:name "has point" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      rdf:type sh:PropertyShape ;
      sh:path AirQualityOntology:hasSegment ;
      sh:minCount 1 ;
      sh:name "has segment" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:targetClass AirQualityOntology:Trajectory ;
  sh:targetObjectsOf AirQualityOntology:hasTrajectory ;
  sh:targetSubjectsOf AirQualityOntology:hasPoint ;
  sh:targetSubjectsOf AirQualityOntology:hasSegment ;
.
AirQualityOntology:carriesPollutant
  rdf:type rdf:Property ;
.
AirQualityOntology:hasObservation
  rdf:type rdf:Property ;
.
AirQualityOntology:hasPoint
  rdf:type rdf:Property ;
.
AirQualityOntology:hasPrescribedStandard
  rdf:type rdf:Property ;
.
AirQualityOntology:hasSegment
  rdf:type rdf:Property ;
.
AirQualityOntology:hasTrajectory
  rdf:type rdf:Property ;
.
