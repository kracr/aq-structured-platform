# baseURI: https://kracr.iiitd.edu.in/ontology/SAQIOntologyPollutionModule
# imports: http://datashapes.org/dash
# prefix: AirQualityOntology

@prefix AirQualityOntology: <https://kracr.iiitd.edu.in/ontology/SAQIOntologyPollutionModule#> .
@prefix dash: <http://datashapes.org/dash#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://kracr.iiitd.edu.in/ontology/SAQIOntologyPollutionModule>
  rdf:type owl:Ontology ;
  owl:imports <http://datashapes.org/dash> ;
  owl:versionInfo "Created with TopBraid Composer" ;


AirQualityOntology:Carrier
  rdf:type rdfs:Class ;
.
AirQualityOntology:CarrierShape
  rdf:type sh:NodeShape ;
  sh:property [
      rdf:type sh:PropertyShape ;
      sh:path AirQualityOntology:carriesPollutant ;
      sh:class AirQualityOntology:Carrier ;
      sh:maxCount 1 ;
      sh:name "carries pollutant" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:targetClass AirQualityOntology:Carrier ;
  sh:targetSubjectsOf AirQualityOntology:carriesPollutant ;
  sh:targetSubjectsOf AirQualityOntology:hasTrajectory ;
.

AirQualityOntology:Observation
  rdf:type rdfs:Class ;
.
AirQualityOntology:ObservationShape
  rdf:type sh:NodeShape ;
  sh:property [
      rdf:type sh:PropertyShape ;
      sh:path AirQualityOntology:atPlace ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      rdf:type sh:PropertyShape ;
      sh:path AirQualityOntology:atTime ;
      sh:datatype xsd:dateTime ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
    ] ;
  sh:targetClass AirQualityOntology:Observation ;
  sh:targetObjectsOf AirQualityOntology:hasObservation ;
.
AirQualityOntology:Pollutant
  rdf:type rdfs:Class ;
.
AirQualityOntology:PollutantShape
  rdf:type sh:NodeShape ;
  sh:property [
      rdf:type sh:PropertyShape ;
      sh:path AirQualityOntology:hasObservation ;
      sh:class AirQualityOntology:Pollutant ;
      sh:minCount 1 ;
      sh:name "has observation" ;
      sh:node AirQualityOntology:ObservationShape ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      rdf:type sh:PropertyShape ;
      sh:path AirQualityOntology:hasPrescribedStandard ;
      sh:class AirQualityOntology:Pollutant ;
      sh:maxCount 1 ;
      sh:name "has prescribed standard" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:targetClass AirQualityOntology:Pollutant ;
  sh:targetObjectsOf AirQualityOntology:carriesPollutant ;
  sh:targetSubjectsOf AirQualityOntology:hasObservation ;
  sh:targetSubjectsOf AirQualityOntology:hasPrescribedStandard ;
.
AirQualityOntology:PrescribedStandardForPollutant
  rdf:type rdfs:Class ;
.
AirQualityOntology:PrescribedStandardForPollutantShape
  rdf:type sh:NodeShape ;
  sh:property [
      rdf:type sh:PropertyShape ;
      sh:path AirQualityOntology:hasObservation ;
      sh:class AirQualityOntology:Observation ;
      sh:minCount 1 ;
      sh:name "has observation" ;
      sh:node AirQualityOntology:ObservationShape ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:targetClass AirQualityOntology:PrescribedStandardForPollutant ;
  sh:targetObjectsOf AirQualityOntology:hasPrescribedStandard ;
.
AirQualityOntology:Trajectory
  rdf:type rdfs:Class ;
.
AirQualityOntology:TrajectorySegment
  rdf:type rdfs:Class ;
.
AirQualityOntology:TrajectorySegmentShape
  rdf:type sh:NodeShape ;
  sh:property [
      rdf:type sh:PropertyShape ;
      sh:path AirQualityOntology:endsAt ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:name "ends at" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:property [
      rdf:type sh:PropertyShape ;
      sh:path AirQualityOntology:startsFrom ;
      sh:maxCount 1 ;
      sh:minCount 1 ;
      sh:name "starts from" ;
      sh:nodeKind sh:IRI ;
    ] ;
  sh:targetClass AirQualityOntology:TrajectorySegment ;
  sh:targetSubjectsOf AirQualityOntology:endsAt ;
  sh:targetSubjectsOf AirQualityOntology:startsFrom ;
.

