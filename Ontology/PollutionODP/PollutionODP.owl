        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#BioChemicalPollutant</IRI>
        <Literal>Biochemical wastes generated from industries</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#BiologicalOrganism</IRI>
        <Literal>Examples include Viruses, pathogens, bacteria or other medium to small pollutant organism which can act as a pollutant above a certain threshold concentration</Literal>
    </AnnotationAssertion>
